<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net6.0</TargetFrameworks>
    <OutputType>Library</OutputType>
    <RootNamespace>Verify.Serilog</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Argon" Version="0.5.0" />
    <Using Remove="System.Net.Http" />
    <PackageReference Include="Serilog" Version="2.12.0" />
    <PackageReference Include="Verify" Version="19.11.1" />
    <PackageReference Include="Polyfill" Version="0.24.0" PrivateAssets="All" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" Condition="$(Configuration) == 'Release'" />
    <PackageReference Include="ProjectDefaults" Version="1.0.90" PrivateAssets="all" />
  </ItemGroup>
</Project>